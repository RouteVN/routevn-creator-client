{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "RouteVN Creator",
  "version": "0.1.0",
  "identifier": "com.routevn.creator",
  "build": {
    "frontendDist": "../_site",
    "devUrl": "http://localhost:3001",
    "beforeDevCommand": "bun run watch:tauri",
    "beforeBuildCommand": "bun run build:tauri",
    "runner": "cargo"
  },
  "app": {
    "windows": [
      {
        "title": "RouteVN Creator",
        "width": 1600,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true,
      "devCsp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
